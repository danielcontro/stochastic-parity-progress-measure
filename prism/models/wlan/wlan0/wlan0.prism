    [] ((s2 = 10 & c2 = 0) & x2 = 0) & (c2 > 0 | c1 > 0) -> (s2' = 2);
    [] (s2 = 11 & x2 = 0) & (c2 > 0 | c1 > 0) -> (s2' = 2);
    [] s2 = 11 & x2 = ACK_TO -> (s2' = 3) & (x2' = 0);
endmodule

rewards "collisions"
    [send1] c1 = 0 & c2 > 0 : 1;
    [send2] c2 = 0 & c1 > 0 : 1;
endrewards

