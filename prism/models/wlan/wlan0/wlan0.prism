    [time] (c1 = 0 & c2 = 2) & (bc1 > 0 | bc2 > 0) : 50 * 1000;
    [time] (c1 = 2 & c2 = 0) & (bc1 > 0 | bc2 > 0) : 50 * 1000;
    [time] (c1 = 2 & c2 = 2) & (bc1 > 0 | bc2 > 0) : 50 * 2000;
endrewards

